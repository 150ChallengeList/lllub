                            local offanim = TweenService:Create(Bindbox1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()
                            ImageLabel_2.Visible = false
                        end)
                    end


                    TextButton_2.MouseButton1Down:Connect(function()
                        state = not state
                        if state then
                            ImageLabel_2.Visible = true
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(255, 255, 255)

                            local offanim = TweenService:Create(ImageLabel_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

