                    BindBoxStrokeThing.Name = "PageButtonBackgroundStroke"
                    BindBoxStrokeThing.Parent = ComboBoxButtonBG
                    BindBoxStrokeThing.Thickness = 1.5
                    BindBoxStrokeThing.Color = Color3.fromRGB(29, 29, 31)

                    local ComboDownBoxStroke = Instance.new("UIStroke")
                    ComboDownBoxStroke.Name = "PageButtonBackgroundStroke"
                    ComboDownBoxStroke.Parent = ComboBoxButtonBG
                    ComboDownBoxStroke.Thickness = 1.5
                    ComboDownBoxStroke.Color = Color3.fromRGB(29, 29, 31)

                    local tableIndex = 1


                    for i, v in next, list do
                        local ComboOptionButton = Instance.new("TextButton")
                        local ComboOptionButtonText = Instance.new("TextLabel")

                        ComboOptionButton.Name = "ComboOptionButton"
                        ComboOptionButton.Parent = ComboDownItemHolder
                        ComboOptionButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        ComboOptionButton.BackgroundTransparency = 1.000
                        ComboOptionButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
                        ComboOptionButton.BorderSizePixel = 0
                        ComboOptionButton.Size = UDim2.new(0, 249, 0, 23)
                        ComboOptionButton.Font = Enum.Font.SourceSans
                        ComboOptionButton.Text = ""
                        ComboOptionButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                        ComboOptionButton.TextSize = 14.000
                        ComboOptionButton.ZIndex = 504

                        ComboOptionButtonText.Name = "ComboOptionButtonText"
                        ComboOptionButtonText.Parent = ComboOptionButton
                        ComboOptionButtonText.BackgroundColor3 = Color3.fromRGB(160, 110, 221)
                        ComboOptionButtonText.BackgroundTransparency = 1.000
                        ComboOptionButtonText.BorderColor3 = Color3.fromRGB(0, 0, 0)
                        ComboOptionButtonText.BorderSizePixel = 0
                        ComboOptionButtonText.Position = UDim2.new(0.0524353273, 0, 0, 0)
                        ComboOptionButtonText.Size = UDim2.new(0, 235, 0, 23)
                        ComboOptionButtonText.Font = Enum.Font.Arial
                        ComboOptionButtonText.Text = v or "Active"
                        ComboOptionButtonText.TextColor3 = Color3.fromRGB(155, 154, 157)
                        ComboOptionButtonText.TextSize = 16.000
                        ComboOptionButtonText.TextXAlignment = Enum.TextXAlignment.Left
                        ComboOptionButtonText.ZIndex = 510

                        local callbacktext = ComboOptionButtonText.Text
                        local state = list[tableIndex]
                        ComboOptionButton.MouseButton1Click:Connect(function()
                            state = ComboOptionButtonText.Text
                            tableIndex = #list
                            for i, v in pairs(ComboDownItemHolder:GetChildren()) do
                                for q, k in pairs(v:GetChildren()) do
                                    if k:IsA("TextLabel") then
                                        k.TextColor3 = Color3.fromRGB(155, 154, 157)
                                    end
                                end
                            end
                            task.spawn(callback, state)
                            ComboIndicatorText.Text = ComboOptionButtonText.Text
                            ComboOptionButtonText.TextColor3 = Color3.fromRGB(160, 110, 221)
                        end)
                    end

                    ComboIndicatorText.Text = list[tableIndex]
                    
                    TextButton.MouseButton1Down:Connect(function()
                        if ComboDownBox.Visible == false then
                            ComboDownBox.Visible = true
                        elseif ComboDownBox.Visible == true then
                            ComboDownBox.Visible = false
                        end
                    end)
                end
                function sectionitems:SliderFloat(name, startval, maxval, callback)
                    local SliderFloatHolder = Instance.new("Frame")
                    local SliderFloatBar = Instance.new("Frame")
                    local SliderFloatBarCorner = Instance.new("UICorner")
                    local SliderFloatFill = Instance.new("Frame")
                    local SliderFloatFillCorner = Instance.new("UICorner")
                    local SliderFloatBalls = Instance.new("Frame")
                    local SliderFloatBallsCorner = Instance.new("UICorner")
                    local SliderFloatButton = Instance.new("TextButton")
                    local InputBoxText = Instance.new("TextLabel")
                    local InputBoxText_2 = Instance.new("TextLabel")


                    SliderFloatHolder.Name = "SliderFloatHolder"
                    SliderFloatHolder.Parent = Items
                    SliderFloatHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderFloatHolder.BackgroundTransparency = 1.000
                    SliderFloatHolder.BorderColor3 = Color3.fromRGB(0, 0, 0)
