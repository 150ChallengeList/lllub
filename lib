                    local tableIndex = 1


                    for i, v in next, list do
                        local ComboOptionButton = Instance.new("TextButton")
                        local ComboOptionButtonText = Instance.new("TextLabel")

                        ComboOptionButton.Name = "ComboOptionButton"
