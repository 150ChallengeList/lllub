                        local offcolor = Color3.fromRGB(26, 25, 28)

                        local offanim = TweenService:Create(DoubleButtonLeft, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                        offanim:Play()
