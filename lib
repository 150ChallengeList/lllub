                            local offcolor = 0

                            local offanim = TweenService:Create(ColorPickerRGBHolder, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
