                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(RGBText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                        end

                        

                        Red:GetPropertyChangedSignal("Text"):Connect(function()
                            r = Red.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)
                        Green:GetPropertyChangedSignal("Text"):Connect(function()
                            g = Green.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)
                        Blue:GetPropertyChangedSignal("Text"):Connect(function()
                            b = Blue.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)

                        
                    end)
                end
