                        task.spawn(callback, DoubleButtonRightButto.Text)
                        local TweenService = game:GetService("TweenService")

                        local offcolor = Color3.fromRGB(16, 15, 17)

                        local offanim = TweenService:Create(DoubleButtonRight, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {BackgroundColor3 = offcolor})

                        offanim:Play()

                        task.wait(0.1)
                        local TweenService = game:GetService("TweenService")

                        local offcolor = Color3.fromRGB(26, 25, 28)

                        local offanim = TweenService:Create(DoubleButtonRight, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                        offanim:Play()
                    end)
                end
