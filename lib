                    ColorPickerButton.Font = Enum.Font.SourceSans
                    ColorPickerButton.Text = ""
                    ColorPickerButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    ColorPickerButton.TextSize = 14.000

                    ColorPickerRGBHolder.Name = "ColorPickerRGBHolder"
                    ColorPickerRGBHolder.Parent = ColorPickerHolder
                    ColorPickerRGBHolder.BackgroundColor3 = Color3.fromRGB(26, 25, 28)
                    ColorPickerRGBHolder.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    ColorPickerRGBHolder.BorderSizePixel = 0
                    ColorPickerRGBHolder.Position = UDim2.new(0, 0, 1.5, 0)
                    ColorPickerRGBHolder.Size = UDim2.new(0, 250, 0, 38)
                    ColorPickerRGBHolder.Visible = false
                    ColorPickerRGBHolder.ZIndex = 500

                    ComboBoxButtonBGCorner.CornerRadius = UDim.new(0, 4)
                    ComboBoxButtonBGCorner.Name = "ComboBoxButtonBGCorner"
                    ComboBoxButtonBGCorner.Parent = ColorPickerRGBHolder

                    local r = Red.Text
                    local g = Green.Text
                    local b = Blue.Text
                    
                    task.spawn(callback, Color3.fromRGB(r, g, b))

                    Blue.Name = "Blue"
                    Blue.Parent = ColorPickerRGBHolder
                    Blue.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    Blue.BackgroundTransparency = 0.850
                    Blue.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Blue.BorderSizePixel = 0
