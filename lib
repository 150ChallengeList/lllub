                    SliderFloatBar.Name = "SliderFloatBar"
                    SliderFloatBar.Parent = SliderFloatHolder
                    SliderFloatBar.BackgroundColor3 = Color3.fromRGB(15, 15, 17)
                    SliderFloatBar.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    SliderFloatBar.BorderSizePixel = 0
