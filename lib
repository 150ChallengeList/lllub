                        ComboOptionButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                        ComboOptionButton.TextSize = 14.000
                        ComboOptionButton.ZIndex = 504

                        ComboOptionButtonText.Name = "ComboOptionButtonText"
                        ComboOptionButtonText.Parent = ComboOptionButton
                        ComboOptionButtonText.BackgroundColor3 = Color3.fromRGB(160, 110, 221)
                        ComboOptionButtonText.BackgroundTransparency = 1.000
                        ComboOptionButtonText.BorderColor3 = Color3.fromRGB(0, 0, 0)
                        ComboOptionButtonText.BorderSizePixel = 0
                        ComboOptionButtonText.Position = UDim2.new(0.0524353273, 0, 0, 0)
                        ComboOptionButtonText.Size = UDim2.new(0, 235, 0, 23)
                        ComboOptionButtonText.Font = Enum.Font.Arial
                        ComboOptionButtonText.Text = v or "Active"
                        ComboOptionButtonText.TextColor3 = Color3.fromRGB(155, 154, 157)
                        ComboOptionButtonText.TextSize = 16.000
                        ComboOptionButtonText.TextXAlignment = Enum.TextXAlignment.Left
                        ComboOptionButtonText.ZIndex = 510

                        local callbacktext = ComboOptionButtonText.Text
                        local state = list[tableIndex]
                        ComboOptionButton.MouseButton1Click:Connect(function()
                            state = ComboOptionButtonText.Text
                            tableIndex = #list
                            for i, v in pairs(ComboDownItemHolder:GetChildren()) do
                                for q, k in pairs(v:GetChildren()) do
                                    if k:IsA("TextLabel") then
                                        k.TextColor3 = Color3.fromRGB(155, 154, 157)
                                    end
                                end
                            end
                            task.spawn(callback, state)
                            ComboIndicatorText.Text = ComboOptionButtonText.Text
                            ComboOptionButtonText.TextColor3 = Color3.fromRGB(160, 110, 221)
                        end)
                    end

                    ComboIndicatorText.Text = list[tableIndex]
                    
                    TextButton.MouseButton1Down:Connect(function()
                        if ComboDownBox.Visible == false then
                            ComboDownBox.Visible = true
                        elseif ComboDownBox.Visible == true then
                            ComboDownBox.Visible = false
                        end
                    end)
                end
