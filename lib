                    BindboxBindButton.MouseButton1Down:Connect(function(a1)
                        BindboxBindButton.Text = "..."
                        
                        local userInputService = game:GetService("UserInputService")

                        local keyPressedEvent = Instance.new("BindableEvent")
                        local isListening = true

                        local function onKeyPress(input, isProcessed)
                            if isListening and input.UserInputType == Enum.UserInputType.Keyboard then
                                isListening = false
                                keyPressedEvent:Fire(input.KeyCode)
                            end
                        end

                        local connection
                        connection = userInputService.InputBegan:Connect(onKeyPress)

                        local keyPressed = keyPressedEvent.Event:Wait()
                        keyPressedEvent:Destroy()

                        print("Key pressed:", keyPressed.Name)
                        BindboxBindButton.Text = keyPressed.Name
                    end)

                    local state = default
                    task.spawn(callback, state, BindboxBindButton.Text)

                    if not state then
                        task.spawn(function()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(18, 18, 18)

                            local offanim = TweenService:Create(ImageLabel_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(BindboxText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(26, 25, 28)

                            local offanim = TweenService:Create(Bindbox1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()
                            ImageLabel_2.Visible = false
                        end)
                    end


                    TextButton_2.MouseButton1Down:Connect(function()
                        state = not state
                        if state then
                            ImageLabel_2.Visible = true
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(255, 255, 255)

                            local offanim = TweenService:Create(ImageLabel_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(246, 245, 248)

                            local offanim = TweenService:Create(BindboxText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(160, 110, 221)

                            local offanim = TweenService:Create(Bindbox1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()
                        elseif not state then
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(18, 18, 18)

                            local offanim = TweenService:Create(ImageLabel_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(BindboxText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(26, 25, 28)

                            local offanim = TweenService:Create(Bindbox1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()
                            ImageLabel_2.Visible = false
                        end
                        task.spawn(callback, state, BindboxBindButton.Text)
                    end)
                end
