                    BindboxBindButton.MouseButton1Down:Connect(function(a1)
                        BindboxBindButton.Text = "..."
                        
                        local userInputService = game:GetService("UserInputService")

                        local keyPressedEvent = Instance.new("BindableEvent")
                        local isListening = true

                        local function onKeyPress(input, isProcessed)
                            if isListening and input.UserInputType == Enum.UserInputType.Keyboard then
                                isListening = false
                                keyPressedEvent:Fire(input.KeyCode)
                            end
                        end

                        local connection
                        connection = userInputService.InputBegan:Connect(onKeyPress)

                        local keyPressed = keyPressedEvent.Event:Wait()
                        keyPressedEvent:Destroy()

                        print("Key pressed:", keyPressed.Name)
                        BindboxBindButton.Text = keyPressed.Name
                    end)

                    local state = default
                    task.spawn(callback, state, BindboxBindButton.Text)

                    if not state then
