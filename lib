                    RGBText.Size = UDim2.new(0, 39, 0, 27)
                    RGBText.Font = Enum.Font.Arial
                    RGBText.Text = "RGB"
                    RGBText.TextColor3 = Color3.fromRGB(111, 110, 113)
                    RGBText.TextSize = 14.000
                    RGBText.ZIndex = 501

                    local checkboxstrokething = Instance.new("UIStroke")
                    checkboxstrokething.Name = "PageButtonBackgroundStroke"
                    checkboxstrokething.Parent = ColorPickerRGBHolder
                    checkboxstrokething.Thickness = 1.5
                    checkboxstrokething.Color = Color3.fromRGB(31, 31, 33)

                    task.spawn(callback, finishedrgb)

                    ColorPickerButton.MouseButton1Down:Connect(function()
                        if ColorPickerRGBHolder.Visible == true then
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0

                            local offanim = TweenService:Create(checkboxstrokething, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Thickness = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 1

                            local offanim = TweenService:Create(ColorPickerRGBHolder, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 1

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 1

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 1

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(22, 22, 24)

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(22, 22, 24)

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(22, 22, 24)

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(22, 22, 24)

                            local offanim = TweenService:Create(RGBText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            task.wait(0.2)
                            ColorPickerRGBHolder.Visible = false
                        elseif ColorPickerRGBHolder.Visible == false then
                            ColorPickerRGBHolder.Visible = true
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 1.5

                            local offanim = TweenService:Create(checkboxstrokething, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Thickness = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0

                            local offanim = TweenService:Create(ColorPickerRGBHolder, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = 0.850

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundTransparency = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Red, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Blue, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(155, 154, 157)

                            local offanim = TweenService:Create(Green, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(RGBText, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()
                        end

                        local r = Red.Text
                        local g = Green.Text
                        local b = Blue.Text

                        Red:GetPropertyChangedSignal("Text"):Connect(function()
                            r = Red.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)
                        Green:GetPropertyChangedSignal("Text"):Connect(function()
                            g = Green.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)
                        Blue:GetPropertyChangedSignal("Text"):Connect(function()
                            b = Blue.Text
                            ColorPickerColorIndicator.BackgroundColor3 = Color3.fromRGB(r, g, b)
                            task.spawn(callback, Color3.fromRGB(r, g, b))
                        end)

                        
                    end)
                end
                function sectionitems:SingleCombo(name, list, callback)
