                    BindboxBindStroke.Name = "PageButtonBackgroundStroke"
                    BindboxBindStroke.Parent = BindboxBindBG
                    BindboxBindStroke.Thickness = 1.5
                    BindboxBindStroke.Color = Color3.fromRGB(31, 31, 33)

