                function sectionitems:Checkbox(CheckboxName, default, callback)
                    local CheckboxOnExample = Instance.new("Frame")
                    local Checkbox1_2 = Instance.new("Frame")
                    local UICorner_3 = Instance.new("UICorner")
                    local ImageLabel = Instance.new("ImageLabel")
                    local TextButton = Instance.new("TextButton")
                    local TextLabel_4 = Instance.new("TextLabel")
                    local checkboxstrokething = Instance.new("UIStroke")
    
                    CheckboxOnExample.Name = "CheckboxOnExample"
                    CheckboxOnExample.Parent = Items
                    CheckboxOnExample.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    CheckboxOnExample.BackgroundTransparency = 1.000
                    CheckboxOnExample.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    CheckboxOnExample.BorderSizePixel = 0
                    CheckboxOnExample.Size = UDim2.new(0, 254, 0, 26)
    
                    Checkbox1_2.Name = "Checkbox1"
                    Checkbox1_2.Parent = CheckboxOnExample
                    Checkbox1_2.BackgroundColor3 = Color3.fromRGB(160, 110, 221)
                    Checkbox1_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Checkbox1_2.BorderSizePixel = 0
                    Checkbox1_2.Size = UDim2.new(0, 26, 0, 26)
    
                    UICorner_3.CornerRadius = UDim.new(0, 4)
                    UICorner_3.Parent = Checkbox1_2
    
                    ImageLabel.Parent = Checkbox1_2
                    ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    ImageLabel.BackgroundTransparency = 1.000
                    ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    ImageLabel.BorderSizePixel = 0
                    ImageLabel.Position = UDim2.new(0.230769247, 0, 0.307692319, 0)
                    ImageLabel.ImageColor3 = Color3.fromRGB(255, 255, 255)
                    ImageLabel.Size = UDim2.new(0, 13, 0, 10)
                    ImageLabel.Image = "rbxassetid://13938488995"
    
                    TextButton.Parent = Checkbox1_2
                    TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextButton.BackgroundTransparency = 1.000
                    TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    TextButton.BorderSizePixel = 0
                    TextButton.Size = UDim2.new(0, 26, 0, 26)
                    TextButton.Font = Enum.Font.SourceSans
                    TextButton.Text = ""
                    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    TextButton.TextSize = 14.000
    
                    TextLabel_4.Parent = CheckboxOnExample
                    TextLabel_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel_4.BackgroundTransparency = 1.000
                    TextLabel_4.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    TextLabel_4.BorderSizePixel = 0
                    TextLabel_4.Position = UDim2.new(0.14173229, 0, 0, 0)
                    TextLabel_4.Size = UDim2.new(0, 217, 0, 26)
                    TextLabel_4.Font = Enum.Font.Arial
                    TextLabel_4.Text = CheckboxName or "Checkbox On"
                    TextLabel_4.TextColor3 = Color3.fromRGB(111, 110, 113)
                    TextLabel_4.TextSize = 18.000
                    TextLabel_4.TextWrapped = true
                    TextLabel_4.TextXAlignment = Enum.TextXAlignment.Left
    
                    checkboxstrokething.Name = "PageButtonBackgroundStroke"
                    checkboxstrokething.Parent = Checkbox1_2
                    checkboxstrokething.Thickness = 1.5
                    checkboxstrokething.Color = Color3.fromRGB(31, 31, 33)
    
                    local state = default
                    task.spawn(callback, state)
    
                    if state then
                        task.spawn(function()
                            ImageLabel.Visible = true

                            checkboxstrokething.Color = Color3.fromRGB(32, 29, 38)


                            TextLabel_4.TextColor3 = Color3.fromRGB(246, 245, 248)


                            ImageLabel.ImageColor3 = Color3.fromRGB(255, 255, 255)


                            Checkbox1_2.BackgroundColor3 = Color3.fromRGB(160, 110, 221)
                        end)
                    elseif not state then
                        task.spawn(function()
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(31, 31, 33)

                            local offanim = TweenService:Create(checkboxstrokething, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Color = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(TextLabel_4, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(18, 18, 18)

                            local offanim = TweenService:Create(ImageLabel, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(26, 25, 28)

                            local offanim = TweenService:Create(Checkbox1_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()

                            ImageLabel.Visible = false
                        end)
                    end
    
                    TextButton.MouseButton1Down:Connect(function()
                        state = not state
                        if state then
                            ImageLabel.Visible = true
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(32, 29, 38)

                            local offanim = TweenService:Create(checkboxstrokething, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Color = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(246, 245, 248)

                            local offanim = TweenService:Create(TextLabel_4, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(255, 255, 255)

                            local offanim = TweenService:Create(ImageLabel, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(160, 110, 221)

                            local offanim = TweenService:Create(Checkbox1_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()
                        elseif not state then
                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(31, 31, 33)

                            local offanim = TweenService:Create(checkboxstrokething, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Color = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(111, 110, 113)

                            local offanim = TweenService:Create(TextLabel_4, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {TextColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(18, 18, 18)

                            local offanim = TweenService:Create(ImageLabel, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

                            offanim:Play()

                            local TweenService = game:GetService("TweenService")

                            local offcolor = Color3.fromRGB(26, 25, 28)

                            local offanim = TweenService:Create(Checkbox1_2, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

                            offanim:Play()

                            ImageLabel.Visible = false
                        end
    
                        task.spawn(callback, state)

                        if not state then
                            ImageLabel.Visible = false
                        end
                    end)
                end
