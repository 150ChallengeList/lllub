        PageButtonBackground.BorderColor3 = Color3.fromRGB(0, 0, 0)
        PageButtonBackground.BorderSizePixel = 0
        PageButtonBackground.Size = UDim2.new(0, 52, 0, 52)
        --PageButtonBackground.BackgroundColor3 = Color3.fromRGB(19, 18, 21)

        PageButtonBackgroundStroke.Name = "PageButtonBackgroundStroke"
        PageButtonBackgroundStroke.Parent = PageButtonBackground
        PageButtonBackgroundStroke.Thickness = 0
        PageButtonBackgroundStroke.Color = Color3.fromRGB(29, 29, 31)
        --PageButtonBackgroundStroke.Thickness = 1

        PageButtonBackgroundCorner.Name = "PageButtonBackgroundCorner"
        PageButtonBackgroundCorner.Parent = PageButtonBackground

        PageButtonIconImage.Name = "PageButtonIconImage"
        PageButtonIconImage.Parent = PageButtonBackground
        PageButtonIconImage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        PageButtonIconImage.BackgroundTransparency = 1.000
        PageButtonIconImage.BorderColor3 = Color3.fromRGB(0, 0, 0)
        PageButtonIconImage.BorderSizePixel = 0
        PageButtonIconImage.Position = UDim2.new(0.115384616, 0, 0.115384616, 0)
        PageButtonIconImage.Size = UDim2.new(0, 40, 0, 40)
        PageButtonIconImage.Image = icon or "rbxassetid://13938226138"
        PageButtonIconImage.ImageColor3 = Color3.fromRGB(113, 113, 115)
        --PageButtonIconImage.ImageColor3 = Color3.fromRGB(160, 110, 221)

        Page.Name = Name.."Page"
        Page.Parent = SkittlzGG
        Page.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Page.BackgroundTransparency = 1.000
        Page.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Page.BorderSizePixel = 0
        Page.Position = UDim2.new(0.185082868, 0, 0.0282828584, 0)
        Page.Size = UDim2.new(0, 575, 0, 431)
        Page.Visible = false

        PageButton.MouseButton1Down:Connect(function(a)
            for _, page in pairs(SkittlzGG:GetChildren()) do
                if page ~= Page and page.Name ~= Name.."Page" and page:IsA("Frame") and page.Name:match("Page") then
                    page.Visible = false
                end
            end
            for _, v in pairs(PageButtonIconHolder:GetChildren()) do
                if v:IsA("TextButton") then
                    for _, k in pairs(v:GetChildren()) do
                        local TweenService = game:GetService("TweenService")

                        local offcolor = Color3.fromRGB(22, 22, 24)

                        local offanim = TweenService:Create(k, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {BackgroundColor3 = offcolor})

                        offanim:Play()
                        for _, y in pairs(k:GetChildren()) do
                            if y:IsA("UIStroke") then
                                local TweenService = game:GetService("TweenService")

                                local offcolor = 0

                                local offanim = TweenService:Create(y, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {Thickness = offcolor})

                                offanim:Play()
                            elseif y:IsA("ImageLabel") then
                                local TweenService = game:GetService("TweenService")

                                local offcolor = Color3.fromRGB(113, 113, 115)

                                local offanim = TweenService:Create(y, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {ImageColor3 = offcolor})

                                offanim:Play()
                            end
                        end
                    end
                end
            end
            Page.Visible = true
            task.wait(0.2)
            local TweenService = game:GetService("TweenService")

            local offcolor = Color3.fromRGB(160, 110, 221)

            local offanim = TweenService:Create(PageButtonIconImage, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {ImageColor3 = offcolor})

            offanim:Play()

            local TweenService = game:GetService("TweenService")

            local offcolor = 1

            local offanim = TweenService:Create(PageButtonBackgroundStroke, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Thickness = offcolor})

            offanim:Play()

            local TweenService = game:GetService("TweenService")

            local offcolor = Color3.fromRGB(19, 18, 21)

            local offanim = TweenService:Create(PageButtonBackground, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {BackgroundColor3 = offcolor})

            offanim:Play()
        end)

        local sections = {}

