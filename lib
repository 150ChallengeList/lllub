    -- Function to handle user input
    local function onInput(input, gameProcessedEvent)
        if gameProcessedEvent then
            return
        end
        
        if input.KeyCode == Enum.KeyCode.Delete then
            if SkittlzUiLib.Parent ~= game:GetService("CoreGui") then
                SkittlzUiLib.Parent = game:GetService("CoreGui")
            elseif SkittlzUiLib.Parent == game:GetService("CoreGui") then
                SkittlzUiLib.Parent = workspace
            end
        end
    end

    -- Connect the input handler function to the input event
    UserInputService.InputBegan:Connect(onInput)

