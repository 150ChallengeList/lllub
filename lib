                    SliderFloatBalls.Parent = SliderFloatBar
                    SliderFloatBalls.BackgroundColor3 = Color3.fromRGB(160, 110, 221)
                    SliderFloatBalls.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    SliderFloatBalls.BorderSizePixel = 0
                    SliderFloatBalls.Position = UDim2.new(0.466403246, 0, -0.400001526, 0)
                    SliderFloatBalls.Size = UDim2.new(0, 18, 0, 18)

                    SliderFloatBallsCorner.CornerRadius = UDim.new(0, 999)
                    SliderFloatBallsCorner.Name = "SliderFloatBallsCorner"
                    SliderFloatBallsCorner.Parent = SliderFloatBalls

                    SliderFloatButton.Name = "SliderFloatButton"
                    SliderFloatButton.Parent = SliderFloatBar
                    SliderFloatButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderFloatButton.BackgroundTransparency = 1.000
                    SliderFloatButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    SliderFloatButton.BorderSizePixel = 0
                    SliderFloatButton.Position = UDim2.new(0.00395256933, 0, -0.200000003, 0)
                    SliderFloatButton.Size = UDim2.new(0, 251, 0, 12)
                    SliderFloatButton.Font = Enum.Font.SourceSans
                    SliderFloatButton.Text = ""
                    SliderFloatButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    SliderFloatButton.TextSize = 14.000

                    InputBoxText.Name = "InputBoxText"
                    InputBoxText.Parent = SliderFloatHolder
                    InputBoxText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    InputBoxText.BackgroundTransparency = 1.000
                    InputBoxText.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    InputBoxText.BorderSizePixel = 0
                    InputBoxText.Position = UDim2.new(0.00393688818, 0, 0, 0)
                    InputBoxText.Size = UDim2.new(0, 191, 0, 22)
                    InputBoxText.Font = Enum.Font.Arial
                    InputBoxText.Text = name or "Slider Float"
                    InputBoxText.TextColor3 = Color3.fromRGB(111, 110, 113)
                    InputBoxText.TextSize = 18.000
                    InputBoxText.TextWrapped = true
                    InputBoxText.TextXAlignment = Enum.TextXAlignment.Left

                    InputBoxText_2.Name = "InputBoxText"
                    InputBoxText_2.Parent = SliderFloatHolder
                    InputBoxText_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    InputBoxText_2.BackgroundTransparency = 1.000
                    InputBoxText_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    InputBoxText_2.BorderSizePixel = 0
                    InputBoxText_2.Position = UDim2.new(0.758893251, 0, 0, 0)
                    InputBoxText_2.Size = UDim2.new(0, 59, 0, 22)
                    InputBoxText_2.Font = Enum.Font.Arial
                    InputBoxText_2.Text = "5.0 ms"
                    InputBoxText_2.TextColor3 = Color3.fromRGB(111, 110, 113)
                    InputBoxText_2.TextSize = 18.000
                    InputBoxText_2.TextWrapped = true
                    InputBoxText_2.TextXAlignment = Enum.TextXAlignment.Right
                end
            else
                print("hi")
            end

            return sectionitems
        end
        return sections
    end
    return pageFunctions
end


local Camera = workspace.CurrentCamera
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local Holding = false

local AimbotEnabled = true
local TeamCheck = false 
local AimPart = "Head"
local Sensitivity = 0.00
local TriggerbotEnabled = false

local CircleSides = 365
local CircleColor = Color3.fromRGB(255, 255, 255)
local CircleTransparency = 0.7
local CircleRadius = 80
local CircleFilled = false 
local CircleVisible = true 
local CircleThickness = 0

local FOVCircleOutline = Drawing.new("Circle")
FOVCircleOutline.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
FOVCircleOutline.Radius = CircleRadius
FOVCircleOutline.Filled = CircleFilled
FOVCircleOutline.Color = Color3.fromRGB(0, 0, 0)
FOVCircleOutline.Visible = CircleVisible
FOVCircleOutline.Radius = CircleRadius
FOVCircleOutline.Transparency = CircleTransparency
FOVCircleOutline.NumSides = CircleSides
FOVCircleOutline.Thickness = CircleThickness + 2

local FOVCircle = Drawing.new("Circle")
FOVCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
FOVCircle.Radius = CircleRadius
FOVCircle.Filled = CircleFilled
FOVCircle.Color = CircleColor
FOVCircle.Visible = CircleVisible
FOVCircle.Radius = CircleRadius
FOVCircle.Transparency = CircleTransparency
FOVCircle.NumSides = CircleSides
FOVCircle.Thickness = CircleThickness + 1

pcall(function()
    local lighting = game:GetService("Lighting");
    lighting.Ambient = Color3.fromRGB(255, 255, 255);
    lighting.Brightness = 1;
    lighting.FogEnd = 1e10;
    for i, v in pairs(lighting:GetDescendants()) do
        if v:IsA("BloomEffect") or v:IsA("BlurEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("SunRaysEffect") then
            v.Enabled = false;
        end;
    end;
    lighting.Changed:Connect(function()
        lighting.Ambient = Color3.fromRGB(255, 255, 255);
        lighting.Brightness = 1;
        lighting.FogEnd = 1e10;
    end);
    spawn(function()
        local character = game:GetService("Players").LocalPlayer.Character;
        while wait() do
            repeat wait() until character ~= nil;
            if not character.HumanoidRootPart:FindFirstChildWhichIsA("PointLight") then
                local headlight = Instance.new("PointLight", character.HumanoidRootPart);
                headlight.Brightness = 1;
                headlight.Range = 60;
            end;
        end;
    end);
end)

return library
